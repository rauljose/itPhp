{
  "require": {
    "php": "^8.2",
    "ext-bcmath": "*",
    "ext-json": "*",

    "akeneo-labs/spreadsheet-parser": "*",
    "composer/ca-bundle": "*",
    "jbroadway/urlify": "*",
    "laminas/laminas-json": "*",
    "mk-j/php_xlsxwriter": "*",
    "ramsey/uuid": "*",
    "spatie/schema-org": "*",
    "spatie/image-optimizer": "*"
  },

  "config": {
    "htaccess-protect":true,
    "prepend-autoloader":false,
    "bin-compat": "full",
    "optimize-autoloader":true,
    "classmap-authoritative": true
  },

  "autoload": {
    "exclude-from-classmap": ["/Tests/", "/test/", "/tests/", "/URLifyTest/"]
  },

  "extras": {
    "oefenweb/damerau-levenshtein": "3.0.*",
    "phpmailer/phpmailer": "~6.0",
    "tecnickcom/tc-lib-barcode": "^1.15.16",
    "khanamiryan/qrcode-detector-decoder": "1.0.2",
    "ralouphie/getallheaders": "*",
    "https://github.com/socialconnect/auth": "*",
    "bartlett/php-compatinfo": "*"
  },
  "extra": {
    "installer-paths": {
      "sites/example.com/modules/{$name}": ["vendor/package"],
      "sites/example.com/themes/{$name}": ["type:drupal-theme"]
    }
  }

}
